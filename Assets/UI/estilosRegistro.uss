}

.buttonReturn {
    width: 100px;
    height: 100px;
    justify-content: flex-start;
    align-self: center;
    align-content: flex-start;
    margin-left: 50px;
    background-image: url("project://database/Assets/UI%20Toolkit/UnityThemes/UnityDefaultRuntimeTheme.tss?fileID=-2236324273849735119&guid=f7bf1eb9a05970c488eebdd2d58f118c&type=3#arrow-left@2x");
