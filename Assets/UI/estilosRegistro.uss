.ContenedorGeneral {
    width: 100%;
    height: 100%;
    align-self: center;
}

.tituloGeneral {
    width: 75%;
    height: 100%;
    align-self: center;
    margin-top: 50px;
    font-size: 100px;
    color: rgb(255, 255, 255);
    -unity-font-definition: url("project://database/Assets/Fonts/PressStart2P-vaV7.ttf?fileID=12800000&guid=eae689eb73f441a47a1f78ca74fe6a68&type=3#PressStart2P-vaV7");
    -unity-text-align: upper-center;
}

.ContenedorRegistro {
    width: 50%;
    align-items: stretch;
    justify-content: space-around;
    align-self: center;
    margin-top: 50px;
    height: 50%;
}

.campos {
    -unity-text-align: upper-center;
    font-size: 20px;
    -unity-font-definition: url("project://database/Assets/Fonts/PressStart2P-vaV7.ttf?fileID=12800000&guid=eae689eb73f441a47a1f78ca74fe6a68&type=3#PressStart2P-vaV7");
